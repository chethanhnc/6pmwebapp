pipeline{
    
    agent any 
    stages{
        stage('SCM checkout'){
            steps{
                git credentialsId: 'master', url: 'https://github.com/chethanhnc/6pmwebapp'

            }
        }
        stage('Build package'){
            steps{
                sh "mvn clean package"

            }
        }
        stage('deploy- to- tomcat'){
            steps{
                echo "deploy into te tomcat"

            }
        }
    }
}